{
  "$schema": "http://json-schema.org/draft-04/hyper-schema#",
  "type": "object",
  "properties": {
    "readme": {
      "type": "string",
      "title": "Mochi 1 Preview",
      "author": "Stability-AI",
      "page": "https://www.genmo.ai/blog",
      "code": "https://github.com/genmoai/models",
      "api": "https://github.com/camenduru/tost-api-examples/blob/main/v1/mochi-1-preview.ipynb",
      "tags": [
        "Text to Video"
      ],
      "widget": "readme"
    },
    "positive_prompt": {
      "type": "string",
      "description": "Prompt",
      "widget": "textarea"
    },
    "negative_prompt": {
      "type": "string",
      "description": "Prompt",
      "widget": "textarea"
    },
    "seed": {
      "type": "integer",
      "description": "Seed (if 0 random)"
    },
    "steps": {
      "type": "integer",
      "widget": "select",
      "description": "Steps",
      "oneOf": [
        {
          "description": "10",
          "enum": [
            10
          ]
        },
        {
          "description": "20",
          "enum": [
            20
          ]
        },
        {
          "description": "30",
          "enum": [
            30
          ]
        }
      ],
      "default": 20
    },
    "cfg": {
      "type": "number",
      "description": "CFG"
    },
    "num_frames": {
      "type": "integer",
      "description": "Num Frames"
    },
    "width": {
      "type": "integer",
      "description": "Width"
    },
    "height": {
      "type": "integer",
      "description": "Height"
    },
    "example": {
      "type": "string",
      "image1": "https://files.catbox.moe/9poovv.mp4",
      "image2": "https://files.catbox.moe/9poovv.mp4",
      "image3": "https://files.catbox.moe/9poovv.mp4",
      "image4": "https://files.catbox.moe/9poovv.mp4",
      "video_aspect_ratio": "75%",
      "widget": "example"
    }
  },
  "buttons": [
    {
      "id": "enter",
